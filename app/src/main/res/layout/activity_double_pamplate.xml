<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DoublePamplate"
    android:background="#FF9800">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="240dp"
        android:src="@drawable/pric"
        />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/a"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:src="@drawable/ia"
                />

            <TextView
                android:id="@+id/aa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/a"
                android:layout_marginLeft="70dp"
                android:text="001"
                android:textAlignment="center"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="70dp"
                android:layout_toRightOf="@+id/a"
                android:src="@drawable/ib" />

            <TextView
                android:id="@+id/ba"
                android:layout_width="93dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/b"
                android:layout_marginStart="-70dp"
                android:layout_marginLeft="-70dp"
                android:layout_marginTop="1dp"
                android:layout_toEndOf="@+id/b"
                android:layout_toRightOf="@+id/b"
                android:text="002" />

            <ImageView
                android:id="@+id/c"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic"
                android:layout_below="@+id/aa"
                android:layout_marginLeft="20dp"
                />

            <TextView
                android:id="@+id/ca"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/c"
                android:layout_marginLeft="60dp"
                android:text="003"
                android:textAlignment="center"
                android:textStyle="bold" />
            <ImageView
                android:id="@+id/d"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/id"
                android:layout_below="@+id/ba"
                android:layout_toRightOf="@+id/c"
                android:layout_marginLeft="80dp"
                />
            <TextView
                android:id="@+id/da"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/d"
                android:layout_marginLeft="190dp"
                android:text="004"
                android:textAlignment="center"
                android:textStyle="bold"
                android:layout_toRightOf="@+id/ca"
                />
            <ImageView
                android:id="@+id/e"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ie"
                android:layout_below="@+id/ca"
                android:layout_marginLeft="20dp"
                />
            <TextView
                android:id="@+id/ea"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/e"
                android:layout_marginLeft="70dp"
                android:text="005"
                android:textAlignment="center"
                android:textStyle="bold" />
            <ImageView
                android:id="@+id/f"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ig"
                android:layout_below="@+id/da"
                android:layout_toRightOf="@+id/e"
                android:layout_marginLeft="70dp"
                />
        </RelativeLayout>
    </ScrollView>


</RelativeLayout>