<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mainActivity1"
    android:orientation="vertical"
    android:background="@color/colorPrimaryDark">
    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="20dp"
                android:elevation="20dp"
                android:padding="500dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                tools:ignore="OnClick">

                <ImageButton
                    android:layout_width="340dp"
                    android:layout_height="200dp"
                    android:src="@drawable/art"
                    android:scaleType="fitXY"
                    android:background="@color/colorPrimary"
                    android:layout_gravity="center"
                    android:onClick="onclick"
                    android:id="@+id/img7"></ImageButton>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="20dp"
                android:elevation="20dp"
                android:padding="500dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp">

                <ImageButton
                    android:layout_width="340dp"
                    android:layout_height="200dp"
                    android:src="@drawable/matt"
                    android:scaleType="fitXY"
                    android:background="@color/colorPrimary"
                    android:layout_gravity="center"
                    android:id="@+id/img8"
                    tools:ignore="InvalidId"></ImageButton>
            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="20dp"
                android:elevation="20dp"
                android:padding="500dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp">
                <ImageButton
                    android:layout_width="340dp"
                    android:layout_height="200dp"
                    android:background="@color/colorPrimary"
                    android:layout_gravity="center"
                    android:src="@drawable/texture"
                    android:scaleType="fitXY"
                    android:id="@+id/img11"></ImageButton></androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="20dp"
                android:elevation="20dp"
                android:padding="500dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp">
                <ImageButton
                    android:layout_width="340dp"
                    android:layout_height="200dp"
                    android:src="@drawable/gsm350"
                    android:scaleType="fitXY"
                    android:background="@color/colorPrimary"
                    android:layout_gravity="center"
                    android:id="@+id/img9"
                    tools:ignore="DuplicateIds"></ImageButton>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="20dp"
                android:elevation="20dp"
                android:padding="500dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp">
                <ImageButton
                    android:layout_width="340dp"
                    android:layout_height="200dp"
                    android:src="@drawable/gsm250"
                    android:background="@color/colorPrimary"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:id="@+id/img10"
                    tools:ignore="DuplicateIds"></ImageButton></androidx.cardview.widget.CardView>



        </LinearLayout>
    </ScrollView>

</LinearLayout>